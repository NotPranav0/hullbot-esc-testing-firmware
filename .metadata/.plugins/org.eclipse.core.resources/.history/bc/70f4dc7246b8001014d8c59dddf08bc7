#include "app.h"
#include "main.h"

void app_init(void) {
	app_main();
}

void app_main(void) {
	while(1) {
		GPIO_PinState user_button_prev = usr_button_read;
		GPIO_PinState usr_button_read = HAL_GPIO_ReadPin(USER_BTN_GPIO_Port, USER_BTN_Pin);
	}
}
