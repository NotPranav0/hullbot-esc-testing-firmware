#ifndef INC_MEAS_CONFIG_H_
#define INC_MEAS_CONFIG_H_

#include <stdint.h>

// no padding bytes.
#pragma pack(push, 1)
typedef struct {
    float    dac1_voltage;
    float    dac2_voltage;
    uint16_t threshold1_raw;
    uint16_t threshold2_raw;
} AppConfig;
#pragma pack(pop)


#endif /* INC_MEAS_CONFIG_H_ */
